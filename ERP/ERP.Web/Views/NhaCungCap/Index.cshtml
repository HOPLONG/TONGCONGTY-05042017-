                            <div class="form-group">
                                <div class="col-md-12">
                                    <div class="col-sm-4">
                                        <input type="text" class="form-control" id="addtencongty" placeholder="Mã nhà cung cấp" ng-model="arraythongtin.ma_nha_cung_cap">
                                    </div>
                                    <div ng-controller="imgCtrl" class="col-md-8">
                                        <div class="col-md-6">
                                            @Html.AntiForgeryToken()
                                            <input type='file' name="files" id="imgInp" style="width: 25%;display: inline-block" />
                                            <input type="submit" value="Upload Ảnh" style="width: 25%;display: inline-block;float: right" />
                                        </div>
                                        <div class="col-md-6">
                                            <img id="blah" src="#" height="100" width="150" />
                                        </div>
                                    </div>
                                </div>                             
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    <div class="col-sm-10">
                                        <input type="text" class="form-control" id="addtencongty" placeholder="Tên nhà cung cấp" ng-model="arraythongtin.ten_nha_cung_cap">
                                    </div>
                                </div>                          
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    <label class="col-sm-2 control-label">Phân loại nhà cung cấp</label>
                                    <select ng-model="arraythongtin.ma_loai_ncc" class="form-control col-sm-3 input-md" style="width: 25% !important">
                                        <option ng-repeat="phanloai in list_phanloai" value="{{phanloai.MA_LOAI_NCC}}">{{phanloai.TEN_LOAI_NCC}}</option>
                                    </select>
                                </div>                              
                            </div>
                            <div class="form-group">
                                <div class="col-md-12">
                                    <label class="col-sm-2 control-label">Loại hàng cung cấp</label>
                                    <div class="col-sm-10">
